{
  "extends": "solhint:recommended",
  "plugins": [],
  "rules": {
    "compiler-version": [ "error", "^0.8.9" ],
    "func-visibility": [ "warn", { "ignoreConstructors": true } ],
    "quotes": [ "error", "single" ],
    "no-inline-assembly": "off",
    "no-empty-blocks": "off",
    "avoid-suicide": "error",
    "avoid-sha3": "warn"
  }
}
